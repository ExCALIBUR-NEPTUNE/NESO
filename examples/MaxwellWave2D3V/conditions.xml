<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <EXPANSIONS>
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ax" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ay" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Az" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Bx" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="By" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Bz" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ex" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ey" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ez" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Jx" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Jy" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Jz" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="phi" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="rho" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ax_minus" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Ay_minus" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="Az_minus" />
        <E COMPOSITE="C[1]" NUMMODES="2" TYPE="MODIFIED" FIELDS="phi_minus" />

    </EXPANSIONS>

    <CONDITIONS>

        <SOLVERINFO>
            <I PROPERTY="EQTYPE" VALUE="MaxwellWavePIC" />
            <I PROPERTY="Projection" VALUE="Continuous" />
        </SOLVERINFO>

        <GLOBALSYSSOLNINFO>
            <V VAR="u">
            <I PROPERTY="GlobalSysSoln" VALUE="IterativeStaticCond" />
            <I PROPERTY="IterativeSolverTolerance" VALUE="1e-8"/>
            </V>
        </GLOBALSYSSOLNINFO>

        <PARAMETERS>
            <P> Lambda = 0.0 </P>
            <P> epsilon = -1.0 </P>
            <P> num_particles_total = 400000 </P>
            <P> num_particles_per_cell = -1 </P>
            <P> particle_time_step = 0.001 </P>
            <P> particle_num_time_steps = 1800 </P>
            <P> particle_num_write_particle_steps = 0 </P>
            <P> particle_num_write_field_energy_steps = 20 </P>
            <P> particle_num_write_field_steps = 0 </P>
            <P> particle_num_print_steps = 40 </P>

            <P> particle_distribution_position_0 = 0 </P>
            <P> particle_distribution_position_1 = 0 </P>
            <P> particle_distribution_position_2 = 0 </P>

            <P>  charge_0 = -1 </P>
            <P>  mass_0 = 1 </P>
            <P>  temperature_ev_0 = 10000 </P>
            <P>  drift_ev_0 = 0.0 </P>
            <P>  pitch_0 = 1.0 </P>
            <P>  number_density_0 = 100 </P>

            <P>  charge_1 = 1 </P>
            <P>  mass_1 = 3672 </P>
            <P>  temperature_ev_1 = 10000 </P>
            <P>  drift_ev_1 = 0.0 </P>
            <P>  pitch_1 = 1.0 </P>
            <P>  number_density_1 = 99 </P>

            <P>  charge_2 = 2 </P>
            <P>  mass_2 = 7344 </P>
            <P>  temperature_ev_2 = 10000 </P>
            <P>  drift_ev_1 = 36000000.0 </P>
            <P>  pitch_2 = -0.55 </P>
            <P>  number_density_2 = 1 </P>

        </PARAMETERS>

        <VARIABLES>
            <E ID="0"> Ax </V>
            <E ID="1"> Ay </V>
            <E ID="2"> Az </V>
            <E ID="3"> Bx </V>
            <E ID="4"> By </V>
            <E ID="5"> Bz </V>
            <E ID="6"> Jz </V>
            <E ID="7"> Ex </V>
            <E ID="8"> Ey </V>
            <E ID="9"> Ez </V>
            <E ID="10"> Jx </V>
            <E ID="11"> Jy </V>
            <E ID="12"> Jy </V>
            <E ID="13"> phi </V>
            <V ID="14"> rho </V>
            <E ID="15"> Ax_minus </V>
            <E ID="16"> Ay_minus </V>
            <E ID="17"> Az_minus </V>
            <E ID="18"> phi_minus </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="1"> C[100] </B>
            <B ID="2"> C[200] </B>
            <B ID="3"> C[300] </B>
            <B ID="4"> C[400] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="1">
                <E VAR="Ax" VALUE="[3]" />
                <E VAR="Ay" VALUE="[3]" />
                <E VAR="Az" VALUE="[3]" />
                <E VAR="Bx" VALUE="[3]" />
                <E VAR="By" VALUE="[3]" />
                <E VAR="Bz" VALUE="[3]" />
                <E VAR="Ex" VALUE="[3]" />
                <E VAR="Ey" VALUE="[3]" />
                <E VAR="Ez" VALUE="[3]" />
                <E VAR="Jx" VALUE="[3]" />
                <E VAR="Jy" VALUE="[3]" />
                <E VAR="Jz" VALUE="[3]" />
                <E VAR="phi" VALUE="[3]" />
                <E VAR="rho" VALUE="[3]" />
                <E VAR="Ax_minus" VALUE="[3]" />
                <E VAR="Ay_minus" VALUE="[3]" />
                <E VAR="Az_minus" VALUE="[3]" />
                <E VAR="phi_minus" VALUE="[3]" />
            </REGION>
            <REGION REF="2">
                <E VAR="Ax" VALUE="[4]" />
                <E VAR="Ay" VALUE="[4]" />
                <E VAR="Az" VALUE="[4]" />
                <E VAR="Bx" VALUE="[4]" />
                <E VAR="By" VALUE="[4]" />
                <E VAR="Bz" VALUE="[4]" />
                <E VAR="Ex" VALUE="[4]" />
                <E VAR="Ey" VALUE="[4]" />
                <E VAR="Ez" VALUE="[4]" />
                <E VAR="Jx" VALUE="[4]" />
                <E VAR="Jy" VALUE="[4]" />
                <E VAR="Jz" VALUE="[4]" />
                <E VAR="phi" VALUE="[4]" />
                <E VAR="rho" VALUE="[4]" />
                <E VAR="Ax_minus" VALUE="[4]" />
                <E VAR="Ay_minus" VALUE="[4]" />
                <E VAR="Az_minus" VALUE="[4]" />
                <E VAR="phi_minus" VALUE="[4]" />
            </REGION>
            <REGION REF="3">
                <E VAR="Ax" VALUE="[1]" />
                <E VAR="Ay" VALUE="[1]" />
                <E VAR="Az" VALUE="[1]" />
                <E VAR="Bx" VALUE="[1]" />
                <E VAR="By" VALUE="[1]" />
                <E VAR="Bz" VALUE="[1]" />
                <E VAR="Ex" VALUE="[1]" />
                <E VAR="Ey" VALUE="[1]" />
                <E VAR="Ez" VALUE="[1]" />
                <E VAR="Jx" VALUE="[1]" />
                <E VAR="Jy" VALUE="[1]" />
                <E VAR="Jz" VALUE="[1]" />
                <E VAR="phi" VALUE="[1]" />
                <E VAR="rho" VALUE="[1]" />
                <E VAR="Ax_minus" VALUE="[1]" />
                <E VAR="Ay_minus" VALUE="[1]" />
                <E VAR="Az_minus" VALUE="[1]" />
                <E VAR="phi_minus" VALUE="[1]" />
            </REGION>
            <REGION REF="4">
                <E VAR="Ax" VALUE="[2]" />
                <E VAR="Ay" VALUE="[2]" />
                <E VAR="Az" VALUE="[2]" />
                <E VAR="Bx" VALUE="[2]" />
                <E VAR="By" VALUE="[2]" />
                <E VAR="Bz" VALUE="[2]" />
                <E VAR="Ex" VALUE="[2]" />
                <E VAR="Ey" VALUE="[2]" />
                <E VAR="Ez" VALUE="[2]" />
                <E VAR="Jx" VALUE="[2]" />
                <E VAR="Jy" VALUE="[2]" />
                <E VAR="Jz" VALUE="[2]" />
                <E VAR="phi" VALUE="[2]" />
                <E VAR="rho" VALUE="[2]" />
                <E VAR="Ax_minus" VALUE="[2]" />
                <E VAR="Ay_minus" VALUE="[2]" />
                <E VAR="Az_minus" VALUE="[2]" />
                <E VAR="phi_minus" VALUE="[2]" />
            </REGION>
        </BOUNDARYCONDITIONS>

    </CONDITIONS>

</NEKTAR>
