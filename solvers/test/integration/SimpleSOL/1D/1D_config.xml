<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
    <FORCING>
        <FORCE TYPE="SourceTerms" />
    </FORCING>

    <CONDITIONS>
        <PARAMETERS>
            <P> TimeStep       = 2e-2        </P>
            <P> NumSteps       = 6250        </P>
            <P> IO_CheckSteps  = NumSteps+1  </P>
            <P> IO_InfoSteps   = NumSteps+1  </P>
            <P> Gamma          = 5.0/3.0     </P>
            <P> GasConstant    = 1.0         </P>
            <P> pInf           = 1.0         </P>
            <P> rhoInf         = 1.0         </P>
            <P> uInf           = 1.0         </P>
        </PARAMETERS>
        <SOLVERINFO>
            <I PROPERTY="EQTYPE"                VALUE="SOL"                  />
            <I PROPERTY="Projection"            VALUE="DisContinuous"        />
            <I PROPERTY="AdvectionType"         VALUE="WeakDG"               />
            <I PROPERTY="TimeIntegrationMethod" VALUE="ClassicalRungeKutta4" />
            <I PROPERTY="UpwindType"            VALUE="ExactToro"            />
            <I PROPERTY="GlobalSysSoln"         VALUE="IterativeStaticCond"  />
        </SOLVERINFO>
        <VARIABLES>
            <V ID="0"> rho </V>
            <V ID="1"> rhou </V>
            <V ID="2"> E </V>
        </VARIABLES>
        <BOUNDARYREGIONS>
            <B ID="0"> C[1] </B>
            <B ID="1"> C[2] </B>
        </BOUNDARYREGIONS>
        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="rho" VALUE="rhoInf" />
                <D VAR="rhou" VALUE="-rhoInf*uInf" />
                <D VAR="E" VALUE="pInf/(Gamma-1)+0.5*rhoInf*(uInf*uInf)" />
            </REGION>
            <REGION REF="1">
                <D VAR="rho" VALUE="rhoInf" />
                <D VAR="rhou" VALUE="rhoInf*uInf" />
                <D VAR="E" VALUE="pInf/(Gamma-1)+0.5*rhoInf*(uInf*uInf)" />
            </REGION>
        </BOUNDARYCONDITIONS>
        <FUNCTION NAME="InitialConditions">
            <F VAR="rho,rhou,E" FILE="ICs.rst" />
        </FUNCTION>
    </CONDITIONS>

</NEKTAR>
